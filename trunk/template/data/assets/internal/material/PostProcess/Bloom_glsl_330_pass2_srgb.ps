#version 330
#extension GL_ARB_separate_shader_objects : require

struct type_CustomBuffer
{
    vec4 _MainTex_TexelSize;
    vec4 _ColorThreshold;
    vec4 _BlurOffsets;
    vec4 _BloomColor;
    float _BloomFactor;
};

uniform type_CustomBuffer CustomBuffer;

uniform sampler2D _MainTex;
uniform sampler2D _BlurTex;

layout(location = 0) in vec2 var_TEXCOORD0;
layout(location = 1) in vec2 var_TEXCOORD1;
layout(location = 0) out vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = texture(_MainTex, var_TEXCOORD0) + ((CustomBuffer._BloomColor * texture(_BlurTex, var_TEXCOORD0)) * CustomBuffer._BloomFactor);
}

