#version 300 es
precision mediump float;
precision highp int;

struct type_CustomBuffer
{
    highp vec4 _MainTex_TexelSize;
    highp vec4 _ColorThreshold;
    highp vec4 _BlurOffsets;
    highp vec4 _BloomColor;
    highp float _BloomFactor;
};

uniform type_CustomBuffer CustomBuffer;

uniform highp sampler2D _MainTex;
uniform highp sampler2D _BlurTex;

in highp vec2 varying_TEXCOORD0;
in highp vec2 var_TEXCOORD1;
layout(location = 0) out highp vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = texture(_MainTex, varying_TEXCOORD0) + ((CustomBuffer._BloomColor * texture(_BlurTex, varying_TEXCOORD0)) * CustomBuffer._BloomFactor);
}

