#version 320 es
precision mediump float;
precision highp int;

struct type_CameraBuffer
{
    highp mat4 MATRIX_V;
    highp mat4 MATRIX_P;
    highp mat4 MATRIX_VP;
    highp mat4 MATRIX_I_VP;
    highp vec3 _WorldSpaceCameraPos;
    highp vec3 _WorldSpaceCameraDir;
    highp vec4 _ProjectionParams;
    highp vec4 _ScreenParams;
    highp vec4 _ZBufferParams;
};

uniform type_CameraBuffer CameraBuffer;

uniform highp sampler2D _MainTex;
uniform highp samplerCube _SkyBox;

layout(location = 0) in highp vec2 var_TEXCOORD0;
layout(location = 1) in highp vec3 var_TEXCOORD1;
layout(location = 2) in highp vec3 var_TEXCOORD2;
layout(location = 0) out highp vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = mix(texture(_MainTex, var_TEXCOORD0), texture(_SkyBox, reflect(normalize(var_TEXCOORD1 - CameraBuffer._WorldSpaceCameraPos), normalize(var_TEXCOORD2))), vec4(0.5));
}

