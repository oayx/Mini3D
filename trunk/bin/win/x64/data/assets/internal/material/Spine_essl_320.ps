#version 320 es
precision mediump float;
precision highp int;

uniform highp sampler2D _MainTex;

layout(location = 0) in highp vec4 var_TEXCOORD0;
layout(location = 1) in highp vec2 var_TEXCOORD1;
layout(location = 0) out highp vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = texture(_MainTex, var_TEXCOORD1) * var_TEXCOORD0;
}

