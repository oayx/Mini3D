#version 320 es
precision mediump float;
precision highp int;

struct type_CustomBuffer
{
    highp vec4 _MainTex_TexelSize;
    highp vec4 _ColorThreshold;
    highp vec4 _BlurOffsets;
    highp vec4 _BloomColor;
    highp float _BloomFactor;
};

uniform type_CustomBuffer CustomBuffer;

uniform highp sampler2D _MainTex;

layout(location = 0) in highp vec2 var_TEXCOORD0;
layout(location = 0) out highp vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = clamp(texture(_MainTex, var_TEXCOORD0) - CustomBuffer._ColorThreshold, vec4(0.0), vec4(1.0));
}

