#version 330
#extension GL_ARB_separate_shader_objects : require

uniform sampler2D _CameraDepthNormalTexture;

layout(location = 0) in vec2 var_TEXCOORD0;
layout(location = 0) out vec4 out_var_SV_Target;

void main()
{
    out_var_SV_Target = vec4(texture(_CameraDepthNormalTexture, var_TEXCOORD0).xyz, 1.0);
}

